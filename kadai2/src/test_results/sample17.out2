program GCMLCM;

var
m , n , a , b , r : integer;
    gcm , lcm : integer;
    
begin
writeln ( STRING );
    readln ( m , n );
    a := m;
    b := n;
    while b <> 0 do
begin
r := a - ( a div b ) * b;
    a := b;
    b := r
end;
gcm := a;
lcm := ( m div gcm ) * n;
writeln ( STRING , gcm , STRING , lcm )
end.
