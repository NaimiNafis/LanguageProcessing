program numberlist;

var
e , n : integer;
    
begin
writeln ( STRING );
    writeln ( STRING , STRING , STRING , STRING , STRING );
    n := 0;
    e := 1;
    while true do
begin
writeln ( n : 6 , STRING , 2 * n : 6 , STRING , n * n : 6 , STRING , n * n * n : 6 , STRING , e : 6 );
    n := n + 1;
    if n >= 16 then break;
    e := e * 2
end
end.
