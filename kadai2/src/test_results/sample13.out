program sample13;

var
x , high , low , mid , can : integer;
    
begin
writeln ( STRING );
    readln ( x );
    if x < 0 then
begin
writeln ( STRING )
endelse
begin
low := 0;
    high := 181;
    while ( high - low ) >= 2 do
begin
mid := ( high + low ) div 2;
    can := mid * mid;
    if x < can then high := mid else if can < x then low := mid else
begin
high := mid;
    low := mid
end
end;
if high = low then writeln ( STRING , x , STRING , low ) else if ( high * high - x ) > ( x - low * low ) then writeln ( STRING , x , STRING , low ) else writeln ( STRING , x , STRING , high )
end
end.
