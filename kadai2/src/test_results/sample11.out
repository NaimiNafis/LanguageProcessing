programNAME ;
 varNAME ,NAME ,NAME :integer;
begin
    writeln(STRING );
    readln(NAME );
    NAME :=NUMBER ;
     whileNAME >NUMBER do
         begin
             readln(NAME );
            NAME :=NAME +NAME ;
             NAME :=NAME -NUMBER end
         ;
         writeln(STRING ,NAME )end
    .
